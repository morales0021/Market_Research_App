{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      },
      "resolveJsonModule": true,
      "checkJs": true,
      "allowJs": true,
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "jsx": "react-jsx", 
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src"]
  }
  